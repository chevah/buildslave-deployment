<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
        <string>chevah.buildslave</string>
    <key>ProgramArguments</key>
        <array>
            <string>/Users/buildslave/buildslave/paver.sh</string>
            <string>debug</string>
        </array>
    <key>WorkingDirectory</key>
        <string>/Users/buildslave/buildslave</string>
    <key>UserName</key>
        <string>buildslave</string>
    <key>KeepAlive</key>
        <dict>
            <key>SuccessfulExit</key>
                <false/>
        </dict>
    <key>ThrottleInterval</key>
        <integer>10</integer>
    <key>StandardOutPath</key>
        <string>/var/log/buildslave/stdout.log</string>
    <key>StandardErrorPath</key>
        <string>/var/log/buildslave/stderr.log</string>
</dict>
</plist>
